    "@chakra-ui/theme": "1.12.2",
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "chakra-ui-markdown-renderer": "4.0.0",
    "chakra-ui-steps-rework-mna": "1.5.0-beta.5",
    "dayjs": "1.11.4",
    "email-validator": "2.0.4",
    "formik": "2.2.9",
    "framer-motion": "4.1.17",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "moment": "2.29.3",
    "query-string": "7.0.1",
    "react": "17.0.2",
    "react-dates": "21.8.0",
    "react-dom": "17.0.2",
    "react-dropzone": "11.3.4",
    "react-helmet": "6.1.0",
    "react-hooks-global-state": "1.0.2",
    "react-notion-x": "4.12.0",
    "react-papaparse": "3.17.1",
    "react-router": "5.2.1",
    "react-router-dom": "5.2.1",
    "react-scripts": "4.0.3",
    "styled-components": "5.3.5",
    "yup": "0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve": "ws --port 3000 -d build --log.format dev --spa index.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stylelint": "stylelint 'src/*'",
    "lint": "yarn prettier && yarn eslint",
    "lint:fix": "yarn prettier:fix && yarn eslint:fix",
    "prettier": "prettier --check 'src/**/*.{jsx,js}'",
    "prettier:fix": "prettier --write 'src/**/*.{jsx,js}'",
    "eslint": "eslint src/**/*.{jsx,js}",
    "eslint:fix": "eslint --fix src/**/*.{jsx,js}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "local-web-server": "5.1.1",
    "prettier": "2.3.2",
    "prop-types": "15.7.2"
  }
}
